#include <stdio.h>

int main() {
    int n, i = 1;
    int t1 = 0, t2 = 1;
    int nextTerm;

    printf("Informe o número de termos: ");
    scanf("%d", &n);

    printf("Sequência de Fibonacci: ");

    do {
        printf("%d ", t1);
        nextTerm = t1 + t2;
        t1 = t2;
        t2 = nextTerm;
        i++;
    } while (i <= n);

    return 0;
}