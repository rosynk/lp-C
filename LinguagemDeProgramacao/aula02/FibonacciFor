#include <stdio.h>

int main() {
    int n, i = 1;
    int t1 = 0, t2 = 1;
    int nextTerm;

    printf("Informe o nÃºmero de termos: ");
    scanf("%d", &n);

    printf("SequÃªncia de Fibonacci: ");

    for (; ;) {
        printf("%d ", t1);
        nextTerm = t1 + t2;
        t1 = t2;
        t2 = nextTerm;
        i++;

     if (i > n) break;

    }

    return 0;
}
